<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/pageBg">

    <View
        android:layout_width="match_parent"
        android:layout_height="15dp"
        android:background="@color/white"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/white">

        <RelativeLayout
            android:id="@+id/img_back"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="11dp">

            <ImageView
                android:layout_width="11dp"
                android:layout_height="20dp"
                android:src="@mipmap/back"
                android:layout_centerInParent="true"/>

        </RelativeLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/trade_record"
            android:textColor="@color/textBlack"
            android:textSize="22sp"
            android:layout_centerInParent="true"/>

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/bg_gray"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:background="@color/white"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/img_out_in"
            android:layout_width="26dp"
            android:layout_height="26dp"
            android:layout_marginLeft="30dp"
            android:layout_gravity="center_vertical"/>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="20dp"
            android:orientation="vertical">
            
            <TextView
                android:id="@+id/tv_out_in"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@color/textBlack"
                android:textSize="23sp"/>

            <TextView
                android:id="@+id/tv_count"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="18sp"
                android:textColor="@color/textBlack"
                android:layout_marginTop="5dp"/>

        </LinearLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp">

            <TextView
                android:id="@+id/tv_confirm"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:visibility="gone"
                android:layout_centerInParent="true"
                android:text="@string/sure_ing"
                android:textColor="@color/cFC"
                android:textSize="15sp"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:background="@drawable/bg_round_text_yellow"/>

            <LinearLayout
                android:id="@+id/tv_finish"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:visibility="gone"
                android:orientation="horizontal"
                android:layout_centerInParent="true"
                android:background="@drawable/bg_round_text_green4">

                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/finish"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="8dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="15sp"
                    android:textColor="@color/c3C"
                    android:text="@string/finish"
                    android:layout_marginRight="10dp"
                    android:layout_gravity="center_vertical"/>

            </LinearLayout>

        </RelativeLayout>
        
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/bg_round_edit_white"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp">

        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="@string/in_address"
            android:textColor="@color/textBlack"
            android:textSize="15sp"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="28dp"/>

        <TextView
            android:id="@+id/tv_receivable"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text=""
            android:textSize="12sp"
            android:textColor="@color/c3C"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="30dp"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:layout_marginTop="6dp"
        android:background="@drawable/bg_round_edit_white"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp">

        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="@string/out_address"
            android:textColor="@color/textBlack"
            android:textSize="15sp"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="28dp"/>

        <TextView
            android:id="@+id/tv_transfer"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text=""
            android:textSize="12sp"
            android:textColor="@color/textBlack"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="30dp"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:layout_marginTop="6dp"
        android:background="@drawable/bg_round_edit_white"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp">

        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="@string/miner_fee"
            android:textColor="@color/textBlack"
            android:textSize="15sp"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="28dp"/>

        <TextView
            android:id="@+id/tv_poundage"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text=""
            android:textSize="12sp"
            android:textColor="@color/textBlack"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="30dp"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:layout_marginTop="6dp"
        android:background="@drawable/bg_round_edit_white"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp">

        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="@string/block_height"
            android:textColor="@color/textBlack"
            android:textSize="15sp"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="28dp"/>

        <TextView
            android:id="@+id/tv_height"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text=""
            android:textSize="12sp"
            android:textColor="@color/textBlack"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="30dp"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:layout_marginTop="6dp"
        android:background="@drawable/bg_round_edit_white"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp">

        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="@string/trade_number"
            android:textColor="@color/textBlack"
            android:textSize="15sp"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="28dp"/>

        <TextView
            android:id="@+id/tv_number"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text=""
            android:textSize="12sp"
            android:textColor="@color/textBlack"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="30dp"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:layout_marginTop="6dp"
        android:background="@drawable/bg_round_edit_white"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp">

        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="@string/trade_type"
            android:textColor="@color/textBlack"
            android:textSize="15sp"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="28dp"/>

        <TextView
            android:id="@+id/tv_type"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text=""
            android:textSize="12sp"
            android:textColor="@color/textBlack"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="30dp"/>

    </LinearLayout>

</LinearLayout>