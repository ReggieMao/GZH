<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SVCircleProgressBar">
        <attr name="svprogress_roundColor" format="color"/>
        <attr name="svprogress_roundProgressColor" format="color"/>
        <attr name="svprogress_roundWidth" format="dimension"></attr>
        <attr name="svprogress_max" format="integer"></attr>
        <attr name="svprogress_style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>

    <declare-styleable name="PayPsdInputView">

        <attr name="maxCount" format="integer" />
        <attr name="circleColor" format="color" />
        <attr name="borderColor" format="color" />
        <attr name="bottomLineColor" format="color" />
        <attr name="radius" format="dimension" />
        <attr name="divideLineWidth" format="dimension" />
        <attr name="divideLineWidth1" format="dimension" />
        <attr name="divideLineColor" format="color" />
        <attr name="rectAngle" format="dimension" />
        <attr name="psdType" format="enum">
            <enum name="weChat" value="0" />
            <enum name="bottomLine" value="1" />
        </attr>
    </declare-styleable>

</resources>